Sub ConditionalFormatting()


Range("C11").Select

    With Range("C11:C16").FormatConditions.Add( _
        Type:=xlExpression, _
        Formula1:="=C11<=C$4")
        
        .Interior.Color = RGB(198, 239, 206)
        .Font.Color = RGB(0, 97, 0)
    
    End With

Range("K11").Select

    With Range("K11:K16").FormatConditions.Add( _
        Type:=xlExpression, _
        Formula1:="=K11<=K$4")
        
        .Interior.Color = RGB(198, 239, 206)
        .Font.Color = RGB(0, 97, 0)
    
    End With

Range("C18").Select

    With Range("C18:C22").FormatConditions.Add( _
        Type:=xlExpression, _
        Formula1:="=C18<=C$4")
        
        .Interior.Color = RGB(198, 239, 206)
        .Font.Color = RGB(0, 97, 0)
    
    End With

Range("K18").Select

    With Range("K18:K22").FormatConditions.Add( _
        Type:=xlExpression, _
        Formula1:="=K18<=K$4")
        
        .Interior.Color = RGB(198, 239, 206)
        .Font.Color = RGB(0, 97, 0)
    
    End With
    
Range("C24").Select

    With Range("C24:C34").FormatConditions.Add( _
        Type:=xlExpression, _
